// navbar标题
#app .navbar a.home-link {
  span {
    display inline !important;
  }
  &::before {
    display: inline-block;
    content: '';
    width: 1.6rem;
    height: 1.6rem;
    background url('/modem-bookmarks/image/favicon.png');
    background-size: 100% 100%;
    margin: 0 .1rem -0.1rem 0;
  }
}

// css copy
::selection{background:$orange; text-shadow:1px 1px #fff;}
::-moz-selection{background:$orange; text-shadow:1px 1px #fff;}
::-webkit-selection{background:$orange; text-shadow:1px 1px #fff;}
